{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "js/",
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "plugins": [
      {"name": "ts-sql-plugin"}
    ]
  },
  "include": [
    "src/**/*.ts"
  ]
}
